---
import "~/styles/index.css";

import Program from "~/components/coaching/program.astro";
import Sub from "~/components/coaching/sub.astro";
import Layout from "../layouts/page.astro";
import Testimonial from "~/components/coaching/testimonial.astro";
import Who from "~/components/coaching/who.astro";
import Intro from "~/components/coaching/intro.astro";
import Contact from "~/components/coaching/contact.astro";
import Dev from "~/components/coaching/dev.astro";
import Faq from "~/components/coaching/faq.astro";

const seo = {
    title: "Programme de coaching | Expert iOS, Développeur Swift, Formateur",
    description:
        "Un accompagnement sur mesure par un expert certifié pour développer vos compétences de développeur iOS.",
};

const navItems = [
  { title: "Programme", url: "#program" },
  { title: "Rejoindre", url: "#sub" },
  { title: "Témoignages", url: "#testimonial" },
  { title: "FAQ", url: "#faq" },
];
---

<Layout seo={seo} nav={navItems}>
    <!-- Intro -->
    <Intro />

    <!-- Programm -->
    <Program />

    <!-- Who -->
    <Who />

    <!-- Dev -->
    <Dev />

    <!--Sub -->
    <Sub />

    <!-- Testimonial -->
    <Testimonial />

    <!-- FAQ -->
    <Faq />

    <!-- Contact -->
    <Contact />
</Layout>
